{
  "name": "boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build-production": "cross-env NODE_ENV=production yarn build",
    "lint": "eslint src --max-warnings=0",
    "test": "jest",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test:watch": "yarn test --watch",
    "generate": "yarn plop --plopfile ./generators/plopfile.js",
    "storybook": "start-storybook -s ./public -p 6007",
    "build-storybook": "build-storybook -s ./public",
    "test:ci": "jest --runInBand",
    "schema:convert": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn --findRelatedTests test --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@babel/preset-typescript": "^7.16.7",
    "@types/lodash.xor": "^4.5.7",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.12.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-react": "^7.30.0",
    "graphql": "^16.6.0",
    "lint-staged": "^12.3.7",
    "lodash.xor": "^4.5.0",
    "next": "12.1.0",
    "next-pwa": "^5.5.2",
    "polished": "^4.2.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.29.0",
    "socket.io-client": "^4.5.1",
    "styled-components": "^5.3.3",
    "styled-icons": "^10.45.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@types/react-slick": "^0.23.8",
    "@types/styled-components": "^5.1.24",
    "apollo": "^2.34.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^2.0.5",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.5.11",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-styled-components": "^7.0.8",
    "plop": "^3.0.5",
    "prettier": "2.5.1",
    "typescript": "^4.6.2"
  }
}
